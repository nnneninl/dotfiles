def_key "+"
	show_clock
def_key "="
	volume_up
def_key "."
	show_lyrics
def_key "U"
	update_database

def_key "f"
	show_browser
def_key "m"
	show_media_library
def_key "P"
	show_playlist
def_key "v"
	show_visualizer
def_key "t"
	show_tag_editor

def_key "j"
	scroll_down
def_key "k"
	scroll_up
def_key "h"
	previous_column
def_key "l"
	next_column
def_key "g"
	move_home
def_key "G"
	move_end

def_key "l"
	play_item
def_key "J"
	move_selected_items_down
def_key "K"
	move_selected_items_up

def_key "n"
	next_found_item
def_key "N"
	previous_found_item
